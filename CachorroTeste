public class CachorroTeste {
// Class Main
	public static void main(String[] args) {
		Cachorro Nick=new Cachorro("Nick", 4);
		Cachorro Mel=new Cachorro("Mel", 7);
		Cachorro Fred=new Cachorro("Fred", 9);

System.out.println(Nick.toString());
Nick.latir();
Nick.correr();

System.out.println(Mel.toString());	
Mel.latir();
Mel.correr();

System.out.println(Fred.toString());
Fred.latir();
Fred.correr();


	}

}
